# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'intripid-dev' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for intripid-dev

  # Pods for GoogleMaps
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  
  # Pods for Firebase
  pod 'Firebase/Core'
  pod 'Firebase/Firestore'
  pod 'Firebase/Storage'

  pod 'Alamofire', '~> 5.0.0-rc.3'
  pod 'SwiftyJSON', '~> 4.0'
  
  # Pods used for selection of multiple photos
  # 2: AssetsPickerVC (https://cocoapods.org/pods/AssetsPickerViewController)
  pod 'AssetsPickerViewController', '~> 2.0'
  pod 'SnapKit', '~> 5.0.0'
  


  # Optionally, include the Swift extensions if you're using Swift.
  pod 'FirebaseFirestoreSwift'

  target 'intripid-devTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'intripid-devUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
